<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.udacity.nanodegree.musicalstructure">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SplashScreenActivity" />
        <activity android:name=".ArtistsActivity" />
        <activity android:name=".NowPlayingActivity" />
        <activity android:name=".SongDetailsActivity" />
        <activity android:name=".GenresActivity" />
        <activity android:name=".SongsListActivity"></activity>
    </application>

</manifest>